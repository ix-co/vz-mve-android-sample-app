<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/run_vzmve_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/run_vzmve_button_label"
        app:layout_constraintBottom_toTopOf="@id/vzmve_cta_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed" />

    <Button
        android:id="@+id/vzmve_cta_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/vzmve_cta_button_label"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/run_vzmve_button" />

</androidx.constraintlayout.widget.ConstraintLayout>
